{
  "name": "marko-jest",
  "version": "1.0.0",
  "description": "Jest Marko transformer & render test utils",
  "main": "index.js",
  "repository": "git@github.com:abiyasa/marko-jest.git",
  "author": "abiyasa <ilham.suhardi@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint ./*.js ./src/**/*.js ./test/**/*.js",
    "test": "jest --no-cache"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "test",
    "marko",
    "testing",
    "test utils",
    "tdd",
    "jest"
  ],
  "peerDependencies": {
    "jsdom": "^11.3.0",
    "marko": "^4.5.6"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.0.4",
    "jsdom": "^11.5.1",
    "marko": "^4.5.6"
  },
  "jest": {
    "transform": {
      "^.+\\.marko$": "<rootDir>/src/preprocessor.js"
    },
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/**/*.marko.js",
      "!test/**",
      "!**/node_modules/**"
    ],
    "coverageDirectory": ".coverage",
    "setupTestFrameworkScriptFile": "<rootDir>/test/jest-setup.js",
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  }
}
